<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                         "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	 <class name="com.iiitb.ooadvoid.pojo.FlopkartItem" table="flopkartItem"  dynamic-update="true">
      
      <id name = "id" type = "int" column = "ID">
         <generator class="native"/>
      </id>
      
      <property name = "Listingid" column = "ListingId" type = "int"  />
      <property name = "CartId" column = "CartId" type = "int"/>
      <property name = "OrderId" column = "OrderId" type = "int"/>
       <property name = "status" column = "status" type = "string"/>
	 
	</class>
</hibernate-mapping>