<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                         "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	 <class name="com.iiitb.ooadvoid.pojo.FlopkartListingDetails" table="FlopkartListingDetails"  dynamic-update="true">
	 
      <meta attribute = "class-description">
         This class contains the Flopkart user detail. 
      </meta>
      
      <id name = "ID" type = "int" column = "ID">
         <generator class="native"/>
      </id>
      <property name = "ListingId" column = "ListingId" type = "int" />
      <property name = "attr_name" column = "attr_name" type = "string" />
      <property name = "attr_val" column = "attr_val" type = "string"/>
     
	</class>
</hibernate-mapping>
